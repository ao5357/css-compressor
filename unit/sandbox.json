	// Special combining based on selectors
	"combineMultiplyDefinedSelectors": {
		"selectors": {
			"test": [
				"#id div.class",
				"#secondary .oops",
				"#today p.boss",
				"#id div.class",
				"#today p.boss",
				"#id div.class",
				"#id div.class",
				"#secondary .oops"
			],
			"expect": [
				"#id div.class",
				"#secondary .oops",
				"#today p.boss"
			]
		},
		"details": {
			"test": [
				"test1;",
				"test2;",
				"test3;",
				"test4;",
				"test5;",
				"test6;",
				"test7;",
				"test8;"
			],
			"expect": [
				"test1;test4;test6;test7;",
				"test2;test8;",
				"test3;test5;"
			]
		}
	},

