{
	// individuals method
	// This test runs simple tests, as there are more extensive tests below, this
	// is just to make sure the central function works as expected
	"individuals": {
		// Directionals/Unit compression
		"Directionals: 4 to 1": {
			"test": "margin:10.0px 10px 10px 10px",
			"expect": "margin:10px"
		},
		"Directionals: 4 to 2": {
			"test": "padding:10px 15px 10.0px 15.0px",
			"expect": "padding:10px 15px"
		},
		"Directionals: 3 to 2": {
			"test": "padding:10px 0.15px 10.0px",
			"expect": "padding:10px .15px"
		},

		// Font-Weight compressions
		"Weight: lighter": {
			"test": "font-weight:lighter",
			"expect": "font-weight:100"
		},
		"Weight: bolder": {
			"test": "font-weight:bolder",
			"expect": "font-weight:900"
		},
		"Weight: heavy": {
			"test": "font-weight:heavy",
			"expect": "font-weight:heavy"
		},

		// Decimal conversions
		"Decimals: 0.8": {
			"test": "opacity:0.8",
			"expect": "opacity:.8"
		},
		"Decimals: Any 0.2983": {
			"test": "any:0.2983",
			"expect": "any:.2983"
		},

		// RGB Conversions (Spaces are removed by inital trim}
		"RGB: single numeric": {
			"test": "color:rgb(145)",
			"expect": "color:#919191"
		},
		"RGB: numeric": {
			"test": "color:rgb(145,123,16)",
			"expect": "color:#917b10"
		},
		"RGB: percentage": {
			"test": "color:rgb(50%,50%,50%)",
			"expect": "color:#7f7f7f"
		},

		// Long name to hex conversions
		"Color to hex": {
			"test": "color:aliceblue",
			"expect": "color:#f0f8ff"
		},

		// Long hex to short name conversions
		"Hex to color": {
			"test": "color:#cd853f",
			"expect": "color:peru"
		},

		// Long hex to short hex conversions
		"Longhex to Shorthex": {
			"test": "color:#aa6600",
			"expect": "color:#a60"
		},

		// None value
		"none value: background": {
			"test": "background:none",
			"expect": "background:0"
		},
		"none value: border": {
			"test": "border:none",
			"expect": "border:0"
		},
		"none value: border-top": {
			"test": "border-top:none",
			"expect": "border-top:0"
		},
		"none value: border-left": {
